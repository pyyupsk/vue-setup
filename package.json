{
  "name": "vue-setup",
  "version": "1.0.0",
  "type": "module",
  "description": "A modern Vue.js starter template with Vite, TypeScript, Tailwind CSS, and best practices built-in.",
  "repository": {
    "type": "git",
    "url": "https://github.com/pyyupsk/vue-setup.git"
  },
  "homepage": "https://github.com/pyyupsk/vue-setup#readme",
  "bugs": {
    "url": "https://github.com/pyyupsk/vue-setup/issues"
  },
  "author": "pyyupsk",
  "license": "MIT",
  "private": true,
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "typescript",
    "tailwindcss",
    "shadcn-vue",
    "starter-template",
    "boilerplate"
  ],
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "dev": "vite",
    "format": "prettier --write . --list-different --cache",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@pyyupsk/vite-plugin-sitemap": "^0.1.0",
    "@tailwindcss/vite": "^4.1.17",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.503.0",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.19.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-pkg": "^0.19.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.48.0",
    "unplugin-unused": "^0.5.6",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.4.1",
    "vue-tsc": "^2.2.12"
  }
}
