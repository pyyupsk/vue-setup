{
  "name": "vue-setup",
  "version": "1.0.0",
  "type": "module",
  "description": "A modern Vue.js starter template with Vite, TypeScript, Tailwind CSS, and best practices built-in.",
  "repository": {
    "type": "git",
    "url": "https://github.com/pyyupsk/vue-setup.git"
  },
  "homepage": "https://github.com/pyyupsk/vue-setup#readme",
  "bugs": {
    "url": "https://github.com/pyyupsk/vue-setup/issues"
  },
  "author": "pyyupsk",
  "license": "MIT",
  "private": true,
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "typescript",
    "tailwindcss",
    "shadcn-vue",
    "starter-template",
    "boilerplate"
  ],
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "dev": "vite",
    "format": "prettier --write . --list-different --cache",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@pyyupsk/vite-plugin-sitemap": "0.2.3",
    "@tailwindcss/vite": "^4.1.18",
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.563.0",
    "reka-ui": "^2.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-pkg": "^0.21.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "unplugin-unused": "^0.5.7",
    "unplugin-vue-router": "^0.19.2",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-pwa": "^1.2.0",
    "vue-tsc": "^3.2.4"
  },
  "overrides": {
    "@types/node": ">=24.0.0 <25.0.0",
    "lodash": ">=4.17.23"
  }
}
